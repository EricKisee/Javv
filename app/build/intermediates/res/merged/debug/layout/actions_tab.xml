<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"  android:weightSum="5" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="bottom">


    <FrameLayout
        android:layout_width="0dp"
        android:id="@+id/fl_profile"
        android:layout_height="wrap_content"
        android:layout_weight="1.00">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/transparent"
            android:id="@+id/btn_profile"
            android:layout_weight="1.00" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            app:srcCompat="@drawable/tab_icon_profile2"
            android:id="@+id/imageView" />
    </FrameLayout>
    <FrameLayout
        android:layout_width="0dp"
        android:id="@+id/fl_story"
        android:layout_height="wrap_content"
        android:layout_weight="1.00">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/transparent"
            android:id="@+id/btn_story"
            android:layout_weight="1.00" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            app:srcCompat="@drawable/tab_icon_story2"
            android:id="@+id/imageView2" />
    </FrameLayout>
    <FrameLayout
        android:layout_width="0dp"
        android:id="@+id/fl_maps"
        android:layout_height="wrap_content"
        android:background="@color/accent"
        android:layout_weight="1.00">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/transparent"
            android:id="@+id/btn_maps"
            android:layout_weight="1.00" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:background="@color/transparent"
            app:srcCompat="@drawable/tab_icon_javv2"
            android:id="@+id/imageView3" />
    </FrameLayout>
    <FrameLayout
        android:layout_width="0dp"
        android:id="@+id/fl_explore"
        android:layout_height="wrap_content"
        android:layout_weight="1.00">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/transparent"
            android:id="@+id/btn_explore"
            android:layout_weight="1.00" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            app:srcCompat="@drawable/tab_icon_explore2"
            android:id="@+id/imageView4" />
    </FrameLayout>
    <FrameLayout
        android:layout_width="0dp"
        android:id="@+id/fl_search"
        android:layout_height="wrap_content"
        android:layout_weight="1.00">
        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="@color/transparent"
            android:id="@+id/btn_search"
            android:layout_weight="1.00" />

        <ImageView
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            app:srcCompat="@drawable/tab_icon_search2"
            android:id="@+id/imageView5" />
    </FrameLayout>

</LinearLayout>